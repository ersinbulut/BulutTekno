            <a class="btn btn-primary" href="~/Login/WriterRegister/">Kayıt Ol</a>
        </li>
        <li class="nav-item">
            <a class="btn btn-primary" href="~/Login/WriterLogin/">Giriş Yap</a>
        </li>*@
                            <li class="login">
                                <button type="button" class="btn login-btn text-uppercase " data-toggle="modal" data-target="#exampleModalCenter">
                                    Giriş Yap
                                </button>
                            </li>
                        </ul>
                    </div>
                </nav>
            </div><!-- end container-fluid -->
        </header><!-- end market-header -->
        <div class="contanier-fluid">
            <!--/Login-->
            <div class="modal fade" id="exampleModalCenter" tabindex="-1" role="dialog" aria-hidden="true">
                <div class="modal-dialog modal-dialog-centered" role="document">
                    <div class="modal-content">
                        <div class="modal-header text-center">
                            <button type="button" class="close" data-dismiss="modal" aria-label="Close">
                                <span aria-hidden="true">&times;</span>
                            </button>
                        </div>
                        <div class="modal-body">

                            <div class="login px-4 mx-auto mw-100">
                                <h5 class="text-center mb-4">Giriş Yap</h5>
                                <form action="/Login/WriterLogin/" method="post">
                                    <div class="form-group">
                                        <label class="mb-2">Eposta</label>
                                        <input type="email" name="WriterMail" class="form-control" id="exampleInputEmail1" aria-describedby="emailHelp" placeholder="info@gmail.com" required="">
                                        <small id="emailHelp" class="form-text text-muted">E-postanızı asla başka biriyle paylaşmayacağız.</small>
                                    </div>
                                    <div class="form-group">
                                        <label class="mb-2">Şifre</label>
                                        <input type="password" name="WriterPassword" class="form-control" id="exampleInputPassword1" placeholder="******" required="">
                                        <small id="emailHelp" class="form-text text-muted"><a href="#">Şifremi Unuttum</a></small>
                                    </div>
                                    <div class="form-check mb-2">
                                        <input type="checkbox" class="form-check-input" id="exampleCheck1" style="margin-left:3px;">
                                        <label class="form-check-label" for="exampleCheck1">Beni Hatırla</label>
                                    </div>
                                    <button type="submit" class="btn btn-primary submit mb-4">Giriş Yap</button>
                                    <p class="text-center pb-4">
                                        <a href="~/AdminLTE/web1/#" data-toggle="modal" data-target="#exampleModalCenter2"> Bir hesabınız yok mu?</a>
                                    </p>
                                </form>
                            </div>
                        </div>

                    </div>
                </div>
            </div>
            <!--//Login-->
            <!--/Register-->
            <div class="modal fade" id="exampleModalCenter2" tabindex="-1" role="dialog" aria-hidden="true">
                <div class="modal-dialog modal-dialog-centered" role="document">
                    <div class="modal-content">
                        <div class="modal-header text-center">
                            <button type="button" class="close" data-dismiss="modal" aria-label="Close">
                                <span aria-hidden="true">&times;</span>
                            </button>
                        </div>
                        <div class="modal-body">
                            <div class="login px-4 mx-auto mw-100">
                                <h5 class="text-center mb-4">Kayıt Ol</h5>
                                <form action="/Login/WriterLogin/" method="post">
                                    <div class="form-group">
                                        <label>Adınız</label>

                                        <input type="text" class="form-control" id="validationDefault01" placeholder="" required="">
                                    </div>
                                    <div class="form-group">
                                        <label>Soyadınız</label>
                                        <input type="text" class="form-control" id="validationDefault02" placeholder="" required="">
                                    </div>

                                    <div class="form-group">
                                        <label class="mb-2">Şifre</label>
                                        <input type="password" class="form-control" id="password1" placeholder="" required="">
                                    </div>
                                    <div class="form-group">
                                        <label>Şifre Tekrar</label>
                                        <input type="password" class="form-control" id="password2" placeholder="" required="">
                                    </div>

                                    <button type="submit" class="btn btn-primary submit mb-4">Kaydol</button>
                                    <p class="text-center pb-4">
                                        <a href="#">
                                            Kaydol a tıklayarak, tüm şartları kabul ediyorum.
                                        </a>
                                    </p>
                                </form>

                            </div>
                        </div>

                    </div>
                </div>
            </div>
            <!--//Register-->

            @RenderBody()
        </div>
        <footer class="footer">
            <div class="container">
                <div class="row">
                    <div class="col-lg-7">
                        <div class="widget">
                            <div class="footer-text text-left">
                                <a href="/Home/HomePage"><span class="text-white" style="font-size:40px;">Bulut Tekno</span></a>
                                <p>Bulut Tekno, pazarlama, haber makalelerini paylaştığımız bir teknoloji blogudur.</p>
                                <div class="social">
                                    <a href="~/BlogTECH/#" data-toggle="tooltip" data-placement="bottom" title="Facebook"><i class="fa fa-facebook"></i></a>
                                    <a href="~/BlogTECH/#" data-toggle="tooltip" data-placement="bottom" title="Twitter"><i class="fa fa-twitter"></i></a>
                                    <a href="~/BlogTECH/#" data-toggle="tooltip" data-placement="bottom" title="Instagram"><i class="fa fa-instagram"></i></a>
                                    <a href="~/BlogTECH/#" data-toggle="tooltip" data-placement="bottom" title="Google Plus"><i class="fa fa-google-plus"></i></a>
                                    <a href="~/BlogTECH/#" data-toggle="tooltip" data-placement="bottom" title="Pinterest"><i class="fa fa-pinterest"></i></a>
                                </div>

