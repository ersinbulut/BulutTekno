@using EntityLayer.Concrete
@model List<Heading>

@{ ViewBag.Title = "HomePage";
    Layout = "~/Views/Shared/_BlogLayout.cshtml"; }

<section class="section first-section">
    <div class="container-fluid">
        <div class="masonry-blog clearfix">
            @foreach (var item in Model)
            {

                if (item.HeadingID == 23)
                {
                    <div class="first-slot">
                        <div class="masonry-box post-media">
                            <img src="@item.ImageUrl" style="width:788px; height:370px;" alt="" class="img-fluid">
                            <div class="shadoweffect">
                                <div class="shadow-desc">
                                    <div class="blog-meta">
                                        <span class="bg-orange"><a href="~/BlogTECH/tech-category-01.html" title="">@item.Category.CategoryName</a></span>
                                        <h4><a href="/Home/BlogDetails/@item.HeadingID" title="">@item.HeadingName</a></h4>
                                        <small><a href="~/BlogTECH/tech-single.html" title="">@item.HeadingDate.ToShortDateString()</a></small>
                                        <small><a href="~/BlogTECH/tech-author.html" title="">@item.Writer.WriterName</a></small>
                                    </div><!-- end meta -->
                                </div><!-- end shadow-desc -->
                            </div><!-- end shadow -->
                        </div><!-- end post-media -->
                    </div><!-- end first-side -->
                }

                if (item.HeadingID == 24)
                {
                    <div class="second-slot">
                        <div class="masonry-box post-media">
                            <img src="@item.ImageUrl" style="width:788px; height:370px;" alt="" class="img-fluid">
                            <div class="shadoweffect">
