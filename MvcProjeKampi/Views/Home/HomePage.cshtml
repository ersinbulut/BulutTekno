                                        <small><a href="~/BlogTECH/tech-single.html" title="">@item.HeadingDate.ToShortDateString()</a></small>
                                        <small><a href="~/BlogTECH/tech-author.html" title="">@item.Writer.WriterName</a></small>
                                    </div><!-- end meta -->
                                </div><!-- end shadow-desc -->
                            </div><!-- end shadow -->
                        </div><!-- end post-media -->
                    </div><!-- end second-side -->
                }


            }
        </div><!-- end masonry -->
    </div>
</section>

